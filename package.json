  "homepage": "https://bitbucket.org/gtmhub/webapp-plugins-sdk#readme",
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/express": "^4.17.13",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
