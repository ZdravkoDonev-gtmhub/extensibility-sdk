{
  "name": "webapp-plugins-sdk",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile-ts": "babel src/main.ts --out-file dist/main.js",
    "watch:sdk": "nodemon --watch main.ts --exec npm run compile-ts",
    "watch:server": "nodemon --watch server.ts --exec ts-node server.ts",
    "start": "concurrently -k -n sdk,server -c magenta,cyan \"npm run watch:sdk\" \"npm run watch:server\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://iliahanev@bitbucket.org/gtmhub/webapp-plugins-sdk.git"
  },
